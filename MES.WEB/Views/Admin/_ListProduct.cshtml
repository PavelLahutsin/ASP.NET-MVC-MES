@model IEnumerable<MES.WEB.Models.ProductVm>




<div class="box box-group">
    <div class="box-header with-border">
        <h3 class="box-title">Список продукции</h3>
    </div>
    <div class="box-body no-padding" style="">
        <ul class="nav nav-stacked">
            @foreach (var item in Model)
            {
            <li ><a style="display: inline-block" onclick="showElement(event, this)" href="@Url.Action("ListStructProduct", new { id = item.Id })">@item.Name</a><span><a  class="text-danger pull-right margin" onclick="deleteElementFromList(event, this)" href="@Url.Action("DeleteProduct", new {id=item.Id} )">Удалить</a></span></li>
            }
        </ul>
    </div>
    <!-- /.box-body -->
</div>

